
%machine foreach {

{
❮list files '/Users/tarvydas/temp/ps/@book-Hamburger Workbench - A Frivolous Introduction to Ohm-JS' ⇉ «files»❯

}
%state idle  {

%on begin {
%proceedto generating
}

}
%state generating  {
❮? «files».empty _next_ idle❯
%send subenv ❮augment env with «files».pop❯
%send kick ❮trigger❯
%nextstate blocked
}
%state blocked  {

%on resume {
%proceedto generating
}

}
}
